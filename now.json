{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next",
      "config": {
        "maxLambdaSize": "20mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/our-team",
      "dest": "/server.js"
    },
    {
      "src": "/catch/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}
