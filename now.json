{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next",
      "config": {
        "maxLambdaSize": "20mb"
      }
    },
    {
      "src": "static/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/catch/(?<id>[^/]*)",
      "dest": "/catch?id=$id"
    },
    {
      "src": "/favicon.ico",
      "dest": "/static/favicon.ico"
    }
  ]
}
